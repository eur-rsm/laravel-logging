{
    "name": "eur-rsm/laravel-logging",
    "description": "Enrich laravel logging",
    "type": "laravel-library",
    "keywords": [
        "laravel",
        "eur",
        "logging",
        "monolog",
        "ECS"
    ],
    "homepage": "https://github.com/eur-rsm/laravel-logging",
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3.0",
        "illuminate/auth": "^10.0 || ^11.0 || ^12.0",
        "illuminate/log": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "EUR\\RSM\\LaravelLogging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EUR\\RSM\\LaravelLogging\\LaravelLoggingServiceProvider"
            ]
        }
    }
}
